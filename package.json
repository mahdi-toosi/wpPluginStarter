{
	"name": "wp-plugin-starter",
	"version": "0.1",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "vite",
		"build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build",
		"build:prod": "cross-env NODE_ENV=production vite build",
		"build:stg": "cross-env NODE_ENV=staging vite build --mode staging",
		"serve": "vite preview",
		"lint": "eslint --ext .js,.ts,.vue .",
		"lint:fix": "yarn lint --fix",
		"lint:markup": "vue-tsc --noEmit",
		"format:check": "prettier .",
		"format:fix": "prettier -u --write .",
		"generate:changelog": "standard-version --skip.commit --skip.tag",
		"release": "standard-version && git push origin main --follow-tags"
	},
	"dependencies": {
		"axios": "^1.2.6",
		"mitt": "^3.0.0",
		"vue": "^3.3.2",
		"vue-router": "^4.0.11"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.15.8",
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@types/node": "^18.8.3",
		"@typescript-eslint/parser": "^5.49.0",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.2",
		"autoprefixer": "^10.3.7",
		"cross-env": "^7.0.3",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^9.14.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.0.3",
		"postcss": "^8.3.9",
		"postcss-html": "^1.5.0",
		"prettier": "^2.4.1",
		"rollup-plugin-analyzer": "^4.0.0",
		"rollup-plugin-visualizer": "^5.9.0",
		"sass": "^1.62.0",
		"standard-version": "^9.3.2",
		"typescript": "^4.9.4",
		"vite": "^4.3.9",
		"vite-plugin-eslint": "^1.8.1",
		"vue-tsc": "^1.6.5"
	}
}
